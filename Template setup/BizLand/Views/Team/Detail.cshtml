@model Team


<main id="main">
    
    <!-- ======= Team Section ======= -->
    <section id="team" class="team section-bg">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>Team</h2>
          <h3>Our Hardworking <span>Team</span></h3>
          <p>Ut possimus qui ut temporibus culpa velit eveniet modi omnis est adipisci expedita at voluptas atque vitae autem.</p>
        </div>

        <div class="row">
           
                  <div class="col-lg-3 col-md-6 d-flex align-items-stretch" data-aos="fade-up" data-aos-delay="100">
                    <div class="Model">
                      <div class="Model-img">
                        <img src="~/assets/img/team/@Model.Image" class="img-fluid" alt="">
                        <div class="social">
                          <a href="">@Html.Raw(Model.Icon1)</a>
                          <a href="">@Html.Raw(Model.Icon2)</a>
                          <a href="">@Html.Raw(Model.Icon3)</a>
                          <a href="">@Html.Raw(Model.Icon4)</a>
                        </div>
                      </div>
                      <div class="Model-info">
                        <h4>@Model.Fullname</a></h4>
                        <span>@Model.Desc</span>
                      </div>
                    </div>
                  </div>
                    
              

        </div>

      </div>
    </section><!-- End Team Section -->
</main>